{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173"
    ],
    "AllowCredentials": true
  },
  "Database": {
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
  "Authentication": {
    "EnableJWT": true,
    "EnableCookie": false,
    "EnableOpenID": false,
    "EnableApiKey": false,
    "EnableImpersonation": true
  },
  "Jwt": {
    "Key": "development-key-for-testing-only-change-in-production",
    "ExpirationMinutes": 1440
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequireNonAlphanumeric": false,
      "RequiredLength": 6
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=meowshare.db"
  },
  "DetailedErrors": true
}